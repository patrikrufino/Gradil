<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!-- Fibonacci-based spacing tokens (example values) -->
    <sys:Double x:Key="Spacing1">8</sys:Double>
    <sys:Double x:Key="Spacing2">13</sys:Double>
    <sys:Double x:Key="Spacing3">21</sys:Double>
    <sys:Double x:Key="Spacing4">34</sys:Double>
    <sys:Double x:Key="Spacing5">55</sys:Double>
</ResourceDictionary>
